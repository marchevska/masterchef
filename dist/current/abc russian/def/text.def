    text(american.intro.2) { str = "Первый раунд нашего" }
    text(american.intro.3) { str = "соревнования проводится" }
    text(american.intro.4) { str = "в ковбойском ресторане" }
    text(american.intro.5) { str = "_Роки Рэнч_. Пусть" }
    text(american.intro.6) { str = "победят сильнейшие!" }
    text(mexican.intro.1) { str = "Авокадо, начос и" }
    text(mexican.intro.2) { str = "кесадии! Приветствую" }
    text(mexican.intro.3) { str = "вас в Мексике, друзья!!" }
