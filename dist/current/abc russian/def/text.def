MasterChef {
    text(def/01.def.intro.title) { str = "Новый гость - Курьер" }
    text(def/01.def.intro.text) { str = "Ожидая нового задания, курьер не против подкрепиться. Наше барбекю будет очень кстати!" }
    text(def/01.def.passed.text) { str = "Хорошая работа! Продолжай в том же духе, и мы обязательно выиграем!" }
    text(def/01.def.expert.text) { str = "Отличная работа! Продолжай в том же духе, и мы обязательно выиграем!" }
    text(def/01.def.failed.text) { str = "Сегодня мы заработали слишком мало! Смотри, какие ингредиенты нужны для рецепта и собирай их побыстрее!" }
    text(def/02.def.intro.title) { str = "Три гостя сразу" }
    text(def/02.def.intro.text) { str = "Тебе удалось справиться с одновременным обслуживанием двух гостей, а теперь мы добавим еще один столик." }
    text(def/02.def.passed.text) { str = "Хорошая работа! А теперь давай посмотрим, как ты справишься с новым типом гостей." }
    text(def/02.def.expert.text) { str = "Отличная работа! А теперь давай посмотрим, как ты справишься с новым типом гостей." }
    text(def/02.def.failed.text) { str = "Сегодня мы заработали слишком мало! Чтобы набрать больше очков, собирай ингредиенты группами большего размера." }
    text(def/03.def.intro.title) { str = "Новый гость - Студентка" }
    text(def/03.def.intro.text) { str = "В местном колледже закончились каникулы, и мы ожидаем наплыв студентов. Студентки - очень терпеливые гости." }
    text(def/03.def.passed.text) { str = "Хорошая работа со студентками. Помни, они никуда не спешат, и ты можешь уделять больше внимания другим гостям." }
    text(def/03.def.expert.text) { str = "Отличная работа со студентками. Помни, они никуда не спешат, и ты можешь уделять больше внимания другим гостям." }
    text(def/03.def.failed.text) { str = "Сегодня мы заработали слишком мало! Следи за настроением гостей - чем оно лучше, тем больше чаевых ты получишь!" }
    text(def/04.def.intro.title) { str = "Еще больше гостей" }
    text(def/04.def.intro.text) { str = "Наш ресторан становится популярным, и каждый день к нам приходит все больше гостей. Все оценят наше новое блюдо - сытную лазанью!" }
    text(def/04.def.passed.text) { str = "Хорошая работа! А завтра ты узнаешь, как поддержать у гостей хорошее настроение." }
    text(def/04.def.expert.text) { str = "Отличная работа! А завтра ты узнаешь, как поддержать у гостей хорошее настроение." }
    text(def/04.def.failed.text) { str = "Сегодня мы заработали слишком мало! Научись обслуживать таких терпеливых гостей, как студентки, ведь дальше будет сложнее." }
    text(def/05.def.intro.title) { str = "Новый бонус - Конфета" }
    text(def/05.def.intro.text) { str = "С помощью конфеты можно немного улучшить настроение гостя, чтобы он не ушел и оставил больше чаевых." }
    text(def/05.def.passed.text) { str = "Хорошая работа! Ты уже умеешь поддерживать настроение гостей, жди более сложных гостей назавтра." }
    text(def/05.def.expert.text) { str = "Отличная работа! Ты уже умеешь поддерживать настроение гостей, жди более сложных гостей назавтра." }
    text(def/05.def.failed.text) { str = "Сегодня мы заработали слишком мало! Давай попробуем еще раз. Если гость начинает сердиться, конфета может помочь." }
    text(def/06.def.intro.title) { str = "Новый гость - Бизнес-леди" }
    text(def/06.def.intro.text) { str = "Всегда в спешке! Старайся обслуживать их как можно быстрее, и тогда можешь рассчитывать на хорошие чаевые." }
    text(def/06.def.passed.text) { str = "Хорошая работа! Непросто обслуживать бизнес-леди, но ты неплохо справляешься с этим!" }
    text(def/06.def.expert.text) { str = "Отличная работа! Непросто обслуживать бизнес-леди, но ты неплохо справляешься с этим!" }
    text(def/06.def.failed.text) { str = "Сегодня мы заработали слишком мало! Непросто обслуживать бизнес-леди, старайся уделять им больше внимания." }
    text(def/07.def.intro.title) { str = "Только дамы" }
    text(def/07.def.intro.text) { str = "В местном университете проводится съезд молодых деловых женщин. Оргкомитет зарезервировал все столики в нашем ресторане для участниц." }
    text(def/07.def.passed.text) { str = "Хорошая работа! Все в восторге от нашей кухни, так что жди еще больше клиентов." }
    text(def/07.def.expert.text) { str = "Отличная работа! Все в восторге от нашей кухни, так что жди еще больше клиентов." }
    text(def/07.def.failed.text) { str = "Сегодня мы заработали слишком мало! Если гость начинает терять терпение, постарайся обслуживать его побыстрее." }
    text(def/08.def.intro.title) { str = "Новый бонус - Стрелка" }
    text(def/08.def.intro.text) { str = "В помощью стрелки ты можешь удалить целый горизонтальный ряд ингредиентов с поля." }
    text(def/08.def.passed.text) { str = "Хорошая работа! Теперь ты можешь избежать переполнения поля." }
    text(def/08.def.expert.text) { str = "Отличная работа! Теперь ты можешь избежать переполнения поля." }
    text(def/08.def.failed.text) { str = "Сегодня мы заработали слишком мало! Не сдавайся! Если поле переполнено, используй бонус, чтобы освободить место." }
    text(def/09.def.intro.title) { str = "Новый гость - Турист" }
    text(def/09.def.intro.text) { str = "Роки Рэнч попал в местный каталог лучших ресторанов, и теперь к нам заходят туристы. Будучи в отпуске, им некуда торопиться." }
    text(def/09.def.passed.text) { str = "Хорошая работа! Еще один день, и мы узнаем имена победителей раунда!" }
    text(def/09.def.expert.text) { str = "Отличная работа! Еще один день, и мы узнаем имена победителей раунда!" }
    text(def/09.def.failed.text) { str = "Сегодня мы заработали слишком мало! Старайся не допускать переполнения поля, иначе потеряешь очки за пропавшие ингредиенты." }
    text(def/10.def.intro.title) { str = "Последний день в Роки Рэнч" }
    text(def/10.def.intro.text) { str = "С нашей помощью ресторан стал очень популярным, и у нас опять не будет отбоя от гостей! Кроме того, сегодня вечером жюри объявит имена победителей." }
    text(def/10.def.passed.text) { str = "Хорошая работа! Первый уже раунд закончен, и сейчас мы услышим имена победителей." }
    text(def/10.def.expert.text) { str = "Отличная работа! Первый уже раунд закончен, и сейчас мы услышим имена победителей." }
    text(def/10.def.failed.text) { str = "Сегодня мы заработали слишком мало! Победа была так близка! Давай попробуем еще раз, и постарайся быть внимательнее." }
    text(def/11.def.intro.title) { str = "Добро пожаловать в Мексику!" }
    text(def/11.def.intro.text) { str = "Мы успешно прошли первый раунд соревнования, и теперь мы в Мексике. Следующие 10 дней мы работаем в ресторане Лемон Лайм." }
    text(def/11.def.passed.text) { str = "Хорошая работа! Первый день прошел неплохо, но это был всего лишь первый день - впереди у тебя еще много работы!" }
    text(def/11.def.expert.text) { str = "Отличная работа! Первый день прошел неплохо, но это был всего лишь первый день - впереди у тебя еще много работы!" }
    text(def/11.def.failed.text) { str = "Сегодня мы заработали слишком мало! Старайся обслуживать бизнес-дам в первую очередь, чтобы получить от них больше чаевых." }
    text(def/12.def.intro.title) { str = "Новый бонус - Корзина" }
    text(def/12.def.intro.text) { str = "Еще один способ избавиться от лишних фишек на поле: этот бонус удаляет ближайшие фишки." }
    text(def/12.def.passed.text) { str = "Хорошая работа! Не забывай использовать корзины, если поле оказывается слишком переполненным." }
    text(def/12.def.expert.text) { str = "Отличная работа! Не забывай использовать корзины, если поле оказывается слишком переполненным." }
    text(def/12.def.failed.text) { str = "Сегодня мы заработали слишком мало! Внимательно следи за игровым полем, не давай ему переполняться!" }
    text(def/13.def.intro.title) { str = "Туристы в новых костюмах" }
    text(def/13.def.intro.text) { str = "Смотри, это наш старый знакомый Турист. Приехав в Мексику, каждый турист покупает местный сувениры - пончо и сомбреро." }
    text(def/13.def.passed.text) { str = "Хорошая работа! Важная информация о туристе: если он натягивает шляпу на глаза - поспеши скорее обслужить его!" }
    text(def/13.def.expert.text) { str = "Отличная работа! Важная информация о туристе: если он натягивает шляпу на глаза - поспеши скорее обслужить его!" }
    text(def/13.def.failed.text) { str = "Сегодня мы заработали слишком мало! Запомни: если турист натягивает шляпу на глаза - время поторопиться с его заказом!" }
    text(def/14.def.intro.title) { str = "Совет по игре" }
    text(def/14.def.intro.text) { str = "Если на поле слишком мало ингредиентов, ты можешь ускорить их появление: нажми на нижнюю область поля, чтобы появился еще один ряд." }
    text(def/14.def.passed.text) { str = "Хорошая работа! Используй ускорение поля осторожно, чтобы не допустить его переполнения." }
    text(def/14.def.expert.text) { str = "Отличная работа! Используй ускорение поля осторожно, чтобы не допустить его переполнения." }
    text(def/14.def.failed.text) { str = "Сегодня мы заработали слишком мало! Не злоупотребляй ускорением поля, чтобы не допустить его переполнения." }
    text(def/15.def.intro.title) { str = "Новый бонус - Прокрутка назад" }
    text(def/15.def.intro.text) { str = "С помощью этого бонуса можно удалить сразу три нижних ряда ингредиентов." }
    text(def/15.def.passed.text) { str = "Хорошая работа! Обратная прокрутка поможет избежать переполнения поля." }
    text(def/15.def.expert.text) { str = "Отличная работа! Обратная прокрутка поможет избежать переполнения поля." }
    text(def/15.def.failed.text) { str = "Сегодня мы заработали слишком мало! Будь внимательнее, не давай полю переполняться." }
    text(def/16.def.intro.title) { str = "Новый гость - Бабушка" }
    text(def/16.def.intro.text) { str = "Добрая бабушка наверняка станет твоим любимым гостем: она очень терпелива и в то же время оставляет большие чаевые." }
    text(def/16.def.passed.text) { str = "Хорошая работа! Бабушки были в восторге от нашего сервиса!" }
    text(def/16.def.expert.text) { str = "Отличная работа! Бабушки были в восторге от нашего сервиса!" }
    text(def/16.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что бабушки и туристы могут ждать своих заказов гораздо дольше, чем бизнес-леди." }
    text(def/17.def.intro.title) { str = "Мужской день!" }
    text(def/17.def.intro.text) { str = "На футбольном стадионе проходит финальный матч. А у нас есть свой сюрприз для болельщиков - _Начос для футбольного фаната_." }
    text(def/17.def.passed.text) { str = "Хорошая работа! Возможно, эти мужчины не так хорошо разбираются в еде, но быстрый сервис нравится всем!" }
    text(def/17.def.expert.text) { str = "Отличная работа! Возможно, эти мужчины не так хорошо разбираются в еде, но быстрый сервис нравится всем!" }
    text(def/17.def.failed.text) { str = "Сегодня мы заработали слишком мало! Давай попробуем еще раз, старайся обслуживать гостей побыстрее." }
    text(def/18.def.intro.title) { str = "Туристы наводнили город!" }
    text(def/18.def.intro.text) { str = "Говорят, что пару дней назад археологи откопали статую ацтекского бога Тлалока. В погоне за сенсацией в город прибыла большая группа туристов." }
    text(def/18.def.passed.text) { str = "Хорошая работа! А теперь мы тоже можем пойти посмотреть на эту статую Тлалока!" }
    text(def/18.def.expert.text) { str = "Отличная работа! А теперь мы тоже можем пойти посмотреть на эту статую Тлалока!" }
    text(def/18.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, туристы всегда очень терпеливы, сделать их довольными совсем несложно." }
    text(def/19.def.intro.title) { str = "День Матери" }
    text(def/19.def.intro.text) { str = "Сегодня отмечается День Матери, и мы ожидаем женщин самых разных возрастов в нашем ресторане!" }
    text(def/19.def.passed.text) { str = "Хорошая работа с дамами! Всем понравилась наша кухня, и они порекомендуют наш ресторан своим друзьям." }
    text(def/19.def.expert.text) { str = "Отличная работа с дамами! Всем понравилась наша кухня, и они порекомендуют наш ресторан своим друзьям." }
    text(def/19.def.failed.text) { str = "Сегодня мы заработали слишком мало! Студентки и бабушки очень терпеливы, так что сперва обслуживай бизнес-леди!" }
    text(def/20.def.intro.title) { str = "Кулинарные Критики" }
    text(def/20.def.intro.text) { str = "Критики лучше всех знают, что такое хороший ресторан. Несколько критиков из жюри пришли оценить нашу работу." }
    text(def/20.def.passed.text) { str = "Хорошая работа! Критикам понравились меню и уровень сервиса. Давай узнаем, кто же победил в этом раунде." }
    text(def/20.def.expert.text) { str = "Отличная работа! Критикам понравились меню и уровень сервиса. Давай узнаем, кто же победил в этом раунде." }
    text(def/20.def.failed.text) { str = "Сегодня мы заработали слишком мало! Будь внимательнее к критикам: без их поддержки нам не победить в этом раунде." }
    text(def/21.def.intro.title) { str = "Добро пожаловать в Россию!" }
    text(def/21.def.intro.text) { str = "Мы успешно прошли два раунда конкурса, и с сегодняшнего дня работаем в русском ресторане _Хлеб да соль_." }
    text(def/21.def.passed.text) { str = "Хорошая работа! Первый день прошел неплохо, но это был всего лишь первый день - впереди у тебя еще много работы!" }
    text(def/21.def.expert.text) { str = "Отличная работа! Первый день прошел неплохо, но это был всего лишь первый день - впереди у тебя еще много работы!" }
    text(def/21.def.failed.text) { str = "Сегодня мы заработали слишком мало! Старайся обслужить каждого гостя, не допускай, чтобы кто-то ушел голодным." }
    text(def/22.def.intro.title) { str = "Туристы в новых костюмах" }
    text(def/22.def.intro.text) { str = "Похоже, туристы решили скупить все сувениры! Традиционные матрешки и косоворотки всегда на пике популярности." }
    text(def/22.def.passed.text) { str = "Хорошая работа! Как видишь, туристы все так же терпеливы, и обслуживать их совсем нетрудно." }
    text(def/22.def.expert.text) { str = "Отличная работа! Как видишь, туристы все так же терпеливы, и обслуживать их совсем нетрудно." }
    text(def/22.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что туристы всегда очень терпеливы, и обслуживать их совсем нетрудно." }
    text(def/23.def.intro.title) { str = "Знаешь ли ты, что..." }
    text(def/23.def.intro.text) { str = "Блюда, похожие на пельмени, готовят во многих странах мира: в Италии - равиоли, в Японии - гедза, в Китае - вонтон." }
    text(def/23.def.passed.text) { str = "Хорошая работа! За последние три дня мы выучили половину рецептов в этом ресторане." }
    text(def/23.def.expert.text) { str = "Отличная работа! За последние три дня мы выучили половину рецептов в этом ресторане." }
    text(def/23.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что курьеров ждет работа, не затягивай их ожидание." }
    text(def/24.def.intro.title) { str = "Новый бонус - Подарок" }
    text(def/24.def.intro.text) { str = "Маленький подарок может значительно улучшить настроение гостя. Хороший совет - давать подарки самым богатым или нетерпеливым гостям." }
    text(def/24.def.passed.text) { str = "Хорошая работа! Помни, что маленький подарок значительно улучшает настроение гостя." }
    text(def/24.def.expert.text) { str = "Отличная работа! Помни, что маленький подарок значительно улучшает настроение гостя." }
    text(def/24.def.failed.text) { str = "Сегодня мы заработали слишком мало! Не забывай давать подарки самым нетерпеливым гостям." }
    text(def/25.def.intro.title) { str = "Встречаем болельщиков" }
    text(def/25.def.intro.text) { str = "Только что закончился финальный матч Молодежного чемпионата по хоккею, и болельщики отмечают победу своей команды в нашем ресторане." }
    text(def/25.def.passed.text) { str = "Хорошая работа с болельщиками! Всем понравились наша кухня и быстрый сервис." }
    text(def/25.def.expert.text) { str = "Отличная работа с болельщиками! Всем понравились наша кухня и быстрый сервис." }
    text(def/25.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что туристы могут ждать дольше, чем остальные гости." }
    text(def/26.def.intro.title) { str = "Новый гость - Вегетарианка" }
    text(def/26.def.intro.text) { str = "Эти девушки совсем не едят мяса, поэтому они такие худенькие." }
    text(def/26.def.passed.text) { str = "Хорошая работа! Запомни, если вегетарианка смотрит на часы - накорми ее поскорее." }
    text(def/26.def.expert.text) { str = "Отличная работа! Запомни, если вегетарианка смотрит на часы - накорми ее поскорее." }
    text(def/26.def.failed.text) { str = "Сегодня мы заработали слишком мало! Если вегетарианка смотрит на часы - накорми ее поскорее!" }
    text(def/27.def.intro.title) { str = "Международный женский день" }
    text(def/27.def.intro.text) { str = "В этот день родственники и друзья дарят женщинам цветы и подарки, и, конечно, многие отмечают праздник в ресторане." }
    text(def/27.def.passed.text) { str = "Хорошая работа с дамами! Всем понравилась наша кухня, и они порекомендуют наш ресторан своим друзьям." }
    text(def/27.def.expert.text) { str = "Отличная работа с дамами! Всем понравилась наша кухня, и они порекомендуют наш ресторан своим друзьям." }
    text(def/27.def.failed.text) { str = "Сегодня мы заработали слишком мало! Обслуживай бизнес-дам в первую очередь, другие гости подождут немного дольше." }
    text(def/28.def.intro.title) { str = "Новый бонус - Перемешивание" }
    text(def/28.def.intro.text) { str = "Этот бонус перемешивая фишки на поле, создавая группы среднего размера." }
    text(def/28.def.passed.text) { str = "Хорошая работа! Теперь ты можешь собирать большие группы ингредиентов, когда это необходимо." }
    text(def/28.def.expert.text) { str = "Отличная работа! Теперь ты можешь собирать большие группы ингредиентов, когда это необходимо." }
    text(def/28.def.failed.text) { str = "Сегодня мы заработали слишком мало! Не забывай перемешивать поле, если не осталось больших групп ингредиентов." }
    text(def/29.def.intro.title) { str = "Новый гость - Школьник" }
    text(def/29.def.intro.text) { str = "Обслуживать детей очень сложно - они так же нетерпеливы, как бизнес-леди, но, в то же время, не ожидай от них больших чаевых." }
    text(def/29.def.passed.text) { str = "Хорошая работа! Это действительно самый сложный гость!" }
    text(def/29.def.expert.text) { str = "Отличная работа! Это действительно самый сложный гость!" }
    text(def/29.def.failed.text) { str = "Сегодня мы заработали слишком мало! Дети выглядят очень мило, но не давай им сбить себя с толку, будь внимательнее." }
    text(def/30.def.intro.title) { str = "Последний день в России" }
    text(def/30.def.intro.text) { str = "Третий раунд завершается, и мы опять ждем критиков, чтобы они оценили нашу работу. Постарайся не разочаровать их!" }
    text(def/30.def.passed.text) { str = "Хорошая работа! И сейчас мы узнаем, кто победил в этом раунде!" }
    text(def/30.def.expert.text) { str = "Отличная работа! И сейчас мы узнаем, кто победил в этом раунде!" }
    text(def/30.def.failed.text) { str = "Сегодня мы заработали слишком мало! Давай попробуем еще раз. Не забывай давать подарки самым нетерпеливым гостям." }
    text(def/31.def.intro.title) { str = "Добро пожаловать в Японию!" }
    text(def/31.def.intro.text) { str = "Говорят, что японские повара творят чудеса из рыбы и риса. Посмотрим, сможешь ли ты освоить это искусство." }
    text(def/31.def.passed.text) { str = "Хорошая работа! Теперь мы знаем, что такое сашими." }
    text(def/31.def.expert.text) { str = "Отличная работа! Теперь мы знаем, что такое сашими." }
    text(def/31.def.failed.text) { str = "Сегодня мы заработали слишком мало! Старайся обслужить каждого гостя, не допускай, чтобы кто-то ушел голодным." }
    text(def/32.def.intro.title) { str = "Рай для вегетарианок" }
    text(def/32.def.intro.text) { str = "В традиционной японской кухне много блюд из рыбы и морепродуктов. Вегетарианки оценят наше новое меню!" }
    text(def/32.def.passed.text) { str = "Хорошая работа! Вегетарианки в восторге от нашего нового меню!" }
    text(def/32.def.expert.text) { str = "Отличная работа! Вегетарианки в восторге от нашего нового меню!" }
    text(def/32.def.failed.text) { str = "Сегодня мы заработали слишком мало! Обращай внимание на детей: они легко выходят из себя, если не накормить их побыстрее." }
    text(def/33.def.intro.title) { str = "Туристы в новых костюмах" }
    text(def/33.def.intro.text) { str = "Большинство туристов успело переодеться в костюмы самураев. Надеюсь, что они никого не поранят своими катанами." }
    text(def/33.def.passed.text) { str = "Хорошая работа! Как видишь, туристы все так же терпеливы, и обслуживать их совсем нетрудно." }
    text(def/33.def.expert.text) { str = "Отличная работа! Как видишь, туристы все так же терпеливы, и обслуживать их совсем нетрудно." }
    text(def/33.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что туристы всегда очень терпеливы, и обслуживать их совсем нетрудно." }
    text(def/34.def.intro.title) { str = "Новый бонус - Ложка" }
    text(def/34.def.intro.text) { str = "Ложка удаляет с поля все ингредиенты выбранного типа. Подними ложку с поля на курсор и нажми на ингредиент." }
    text(def/34.def.passed.text) { str = "Хорошая работа! Не допускай переполнения поля, удаляй лишние ингредиенты, если их слишком много." }
    text(def/34.def.expert.text) { str = "Отличная работа! Не допускай переполнения поля, удаляй лишние ингредиенты, если их слишком много." }
    text(def/34.def.failed.text) { str = "Сегодня мы заработали слишком мало! Не допускай переполнения поля, удаляй лишние ингредиенты, если их слишком много." }
    text(def/35.def.intro.title) { str = "Опять спортивные болельщики" }
    text(def/35.def.intro.text) { str = "Завтра начинается чемпионат по сумо, поэтому повсюду видны болельщики и туристы. Помни, что быстрый сервис сохраняет у гостей хорошее настроение!" }
    text(def/35.def.passed.text) { str = "Хорошая работа с болельщиками! Всем понравились наша кухня и быстрый сервис." }
    text(def/35.def.expert.text) { str = "Отличная работа с болельщиками! Всем понравились наша кухня и быстрый сервис." }
    text(def/35.def.failed.text) { str = "Сегодня мы заработали слишком мало! Обращай внимание на детей: они легко выходят из себя, если не накормить их побыстрее." }
    text(def/36.def.intro.title) { str = "Любители манги в городе" }
    text(def/36.def.intro.text) { str = "Вся молодежь только и говорит о начинающемся фестивале манги и аниме. Мне тоже удалось достать билетик на открытие фестиваля сегодня вечером." }
    text(def/36.def.passed.text) { str = "Хорошая работа! День закончен, и я могу пойти смотреть открытие фестиваля!" }
    text(def/36.def.expert.text) { str = "Отличная работа! День закончен, и я могу пойти смотреть открытие фестиваля!" }
    text(def/36.def.failed.text) { str = "Сегодня мы заработали слишком мало! Дети выглядят очень мило, но не давай им сбить себя с толку, будь внимательнее." }
    text(def/37.def.intro.title) { str = "Новый бонус - Сердечко" }
    text(def/37.def.intro.text) { str = "Сердечко улучшает настроение сразу всем гостям." }
    text(def/37.def.passed.text) { str = "Хорошая работа! Теперь ты можешь улучшить настроение всем гостям сразу." }
    text(def/37.def.expert.text) { str = "Отличная работа! Теперь ты можешь улучшить настроение всем гостям сразу." }
    text(def/37.def.failed.text) { str = "Сегодня мы заработали слишком мало! Не забывай следить за настроением гостей!" }
    text(def/38.def.intro.title) { str = "Обожаю распродажи!" }
    text(def/38.def.intro.text) { str = "Все женщины так ждали этого! В знаменитом ГигаМарте началась сезонная распродажа! И ничто так не возбуждает аппетит, как удачные покупки." }
    text(def/38.def.passed.text) { str = "Хорошая работа с дамами! Всем понравилась наша кухня, и они порекомендуют наш ресторан своим друзьям." }
    text(def/38.def.expert.text) { str = "Отличная работа с дамами! Всем понравилась наша кухня, и они порекомендуют наш ресторан своим друзьям." }
    text(def/38.def.failed.text) { str = "Сегодня мы заработали слишком мало! Обслуживай бизнес-дам в первую очередь, другие гости подождут немного дольше." }
    text(def/39.def.intro.title) { str = "Осталось два дня" }
    text(def/39.def.intro.text) { str = "До финала соревнования остается всего два дня, и критики из жюри решили удвоить свои усилия. К счастью, наш ресторан также популярен у туристов!" }
    text(def/39.def.passed.text) { str = "Хорошая работа! Довольные бабушки расскажут о наc всем своим друзьям, так что без работы мы не останемся!" }
    text(def/39.def.expert.text) { str = "Отличная работа! Довольные бабушки расскажут о наc всем своим друзьям, так что без работы мы не останемся!" }
    text(def/39.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что бабушки и туристы могут ждать дольше, обслуживай критиков в первую очередь." }
    text(def/40.def.intro.title) { str = "Жду-не дождусь результатов" }
    text(def/40.def.intro.text) { str = "Последний день в Суши Сайд, и сегодня вечером жюри назовет финалистов. Мы должны постараться, чтобы попасть в финал!" }
    text(def/40.def.passed.text) { str = "Хорошая работа! И кто же победил?" }
    text(def/40.def.expert.text) { str = "Отличная работа! И кто же победил?" }
    text(def/40.def.failed.text) { str = "Сегодня мы заработали слишком мало! Будь внимательнее к критикам: без их поддержки нам не пройти в финал." }
    text(def/41.def.intro.title) { str = "Начинается последний раунд" }
    text(def/41.def.intro.text) { str = "Множество людей приезжает в отпуск на Гавайи, так что недостаток клиентов нам не грозит. И еще, моя мечта как никогда близка к осуществлению." }
