    text(def/42.def.intro.text) { str = "Начинается сезон летних отпусков, и туристы со всего мира приезжают сюда." }
    text(def/42.def.passed.text) { str = "Хорошая работа! Довольные туристы привлекут еще больше гостей в наш ресторан!" }
    text(def/42.def.expert.text) { str = "Отличная работа! Довольные туристы привлекут еще больше гостей в наш ресторан!" }
    text(def/42.def.failed.text) { str = "Сегодня мы заработали слишком мало! Помни, что туристы всегда очень терпеливы, и обслуживать их совсем нетрудно." }
    text(def/43.def.intro.title) { str = "Начинается фестиваль цирка" }
    text(def/43.def.intro.text) { str = "Фестиваль привлек множество поклонников этого искусства. Мы спонсировали шоу юных гимнастов, и ждем сегодня в гости много молодежи." }
    text(def/43.def.passed.text) { str = "Хорошая работа! День закончен, и сегодня вечером мы идем смотреть цирковое шоу." }
