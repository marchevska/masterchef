MasterChef {
    IngredientTypes {
        Type { name = "meat" }
        Type { name = "fish" }
        Type { name = "vegetable" }
        Type { name = "fruit" }
        Type { name = "pasta" }
        Type { name = "dairy" }
        Type { name = "sweet" }
        Type { name = "spice" }
        Type { name = "cereal" }
        Type { name = "egg" }
        Type { name = "soy" }
    }
    
    Ingredients {
        //American
        Ingredient(meat) { type = "meat" src = "ingredient.meat" iconSrc = "ingredient.meat.icon"}
        Ingredient(milk) { type = "dairy" src = "ingredient.milk" iconSrc = "ingredient.milk.icon"}
        Ingredient(pasta) { type = "pasta" src = "ingredient.pasta" iconSrc = "ingredient.pasta.icon"}
        Ingredient(pineapple) { type = "fruit" src = "ingredient.pineapple" iconSrc = "ingredient.pineapple.icon"}
        Ingredient(pumpkin) { type = "vegetable" src = "ingredient.pumpkin" iconSrc = "ingredient.pumpkin.icon"}
        Ingredient(tomato) { type = "vegetable" src = "ingredient.tomato" iconSrc = "ingredient.tomato.icon" }
        
        //Mexican
        Ingredient(pork) { type = "meat" src = "ingredient.pork" iconSrc = "ingredient.pork.icon"}
        Ingredient(chili-pepper) { type = "vegetable" src = "ingredient.chili-pepper" iconSrc = "ingredient.chili-pepper.icon" }
        Ingredient(avocado) { type = "vegetable" src = "ingredient.avocado" iconSrc = "ingredient.avocado.icon" }
        Ingredient(cheese) { type = "dairy" src = "ingredient.cheese" iconSrc = "ingredient.cheese.icon"}
        Ingredient(tortilla) { type = "pasta" src = "ingredient.tortilla" iconSrc = "ingredient.tortilla.icon"}
        
        //Russian
        Ingredient(mushroom) { type = "vegetable" src = "ingredient.mushroom" iconSrc = "ingredient.mushroom.icon"}
        Ingredient(cabbage) { type = "vegetable" src = "ingredient.cabbage" iconSrc = "ingredient.cabbage.icon"}
        Ingredient(potato) { type = "vegetable" src = "ingredient.potato" iconSrc = "ingredient.potato.icon"}
        Ingredient(onion) { type = "vegetable" src = "ingredient.onion" iconSrc = "ingredient.onion.icon" }
        Ingredient(flour) { type = "pasta" src = "ingredient.flour" iconSrc = "ingredient.flour.icon"}
        
        //Japanese
        Ingredient(salmon) { type = "fish" src = "ingredient.salmon" iconSrc = "ingredient.salmon.icon"}
        Ingredient(shrimp) { type = "fish" src = "ingredient.shrimp" iconSrc = "ingredient.shrimp.icon"}
        Ingredient(rice) { type = "cereal" src = "ingredient.rice" iconSrc = "ingredient.rice.icon"}
        Ingredient(tofu) { type = "soy" src = "ingredient.tofu" iconSrc = "ingredient.tofu.icon"}
        Ingredient(veggymix) { type = "vegetable" src = "ingredient.veggymix" iconSrc = "ingredient.veggymix.icon"}
        Ingredient(seaweed) { type = "vegetable" src = "ingredient.seaweed" iconSrc = "ingredient.seaweed.icon"}
        
        //Hawaiian
        Ingredient(lime) { type = "fruit" src = "ingredient.lime" iconSrc = "ingredient.lime.icon"}
        Ingredient(mango) { type = "fruit" src = "ingredient.mango" iconSrc = "ingredient.mango.icon"}
        Ingredient(shoyu) { type = "spice" src = "ingredient.shoyu" iconSrc = "ingredient.shoyu.icon"}
        
        //Bonus tokens on the field
        Ingredient(bonus.bomb) { type = "bonus" src = "bonus.bomb" }
        Ingredient(bonus.gift) { type = "bonus" src = "bonus.gift" }
        Ingredient(bonus.hearts) { type = "bonus" src = "bonus.hearts" }
        Ingredient(bonus.magicwand) { type = "bonus" src = "bonus.magicwand" }
        Ingredient(bonus.removerow) { type = "bonus" src = "bonus.removerow" }
        Ingredient(bonus.scrollback) { type = "bonus" src = "bonus.scrollback" }
        Ingredient(bonus.shuffle) { type = "bonus" src = "bonus.shuffle" }
        Ingredient(bonus.spoon) { type = "bonus" src = "bonus.spoon" }
        Ingredient(bonus.sweet) { type = "bonus" src = "bonus.sweet" }
    }
    
}
