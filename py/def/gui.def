//---------------------------------------------------------------------
// "EpisodeIntro" Dialog 
//---------------------------------------------------------------------
    
    Dialog(EpisodeIntro) {
        layer = "60"
        music = "Game"
        sublayerBase = "-5"
        Parameter(MaxPeopleOnLevel) { type = "integer" value = "8" }
        Variant(Theme) {
            Repeat{
                Variable(Episode) { values = "'American', 'Mexican', 'Russian', 'Japanese', 'Hawaiian'" }
                Node {
                    Case($Episode$) {
                        Image(Background) { sprite = "'theme.'+string.lower('$Episode$')+'.background'" sublayer = "5" }
                        Image(IntroPane) { sprite = "'theme.'+string.lower('$Episode$')+'.intro-pane'" sublayer = "4" x = "490" y = "18" }
                        TextLabel(Title) { style = "TextLabel.Domcasual-10-Center" x = "360" y = "35" textDefault = "string.upper('$Episode$')+' OPEN'" }
                        Repeat {
                            Variable(i) { start = "1" finish = "7" step = "1" }
                            Node {
                                TextLabel(IntroSpeech$i$) { style = "TextLabel.EpisodeIntro.Text" x = "270" y = "275+25*$i$" textDefault = "string.lower('$Episode$')+'.intro.'+str($i$)" }
                            }
                        }
                    }
                }
            }
        }
        Image(Logo) { sprite = "intro.logo" sublayer = "1" x = "6" y = "1" }
        Image(Presenter) { sprite = "comics.presenter" sublayer = "3" x = "460" y = "520" hotspot = "CenterBottom" }
        Image(Balloon) { sprite = "intro.balloon" sublayer = "1" x = "270" y = "370" hotspot = "Center" }
        TextLabel(Competitors) { style = "TextLabel.EpisodeIntro.Competitors" x = "640" y = "273" textDefault = "Str_IntroCompetitors" }
        Repeat {
            Variable(i) { start = "0" finish = "7" step = "1" }
            Node {
                Image(Character$i$)
                TextLabel(CharacterName$i$) { style = "TextLabel.EpisodeIntro.Names" x = "640" y = "315+30*$i$" }
            }
        }
        PushButton(Close) {
            style = "PushButton.WholeScreen" sublayer = "1"
        }
        PushButton(Continue) {
            style = "PushButton.Continue" x = "730" y = "578"
        }
    }
    
//---------------------------------------------------------------------
// "EpisodeOutro" Dialog 
//---------------------------------------------------------------------
    
    Dialog(EpisodeOutro) {
        layer = "60"
        music = "Game"
        sublayerBase = "-5"
        Parameter(MaxPeopleOnLevel) { type = "integer" value = "8" }
        Parameter(MaxPeoplePassingFurther) { type = "integer" value = "4" }
        Image(Logo) { sublayer = "2" sprite = "outro.bg-pane" x = "37" y = "18" }
        TextLabel(Competitors) { style = "TextLabel.Domcasual-10-Center" x = "570" y = "270" textDefault = "Str_OutroCompetitors" }
        TextLabel(Points) { style = "TextLabel.Domcasual-10-Center" x = "710" y = "270" textDefault = "Str_OutroPoints" }
        Image(Separator) { sprite = "outro.separator" sublayer = "3" x = "670" y = "254" }
        Image(Presenter) { sprite = "comics.presenter" sublayer = "3" x = "460" y = "520" hotspot = "CenterBottom" }
        Image(Balloon) { sprite = "outro.balloon" sublayer = "1" x = "220" y = "440" hotspot = "Center" }
        
        Variant(Theme) {
            Repeat{
                Variable(Episode) { values = "'American', 'Mexican', 'Russian', 'Japanese', 'Hawaiian'" }
                Node {
                    Case($Episode$) {
                        Image(Background) { sprite = "'theme.'+string.lower('$Episode$')+'.background'" sublayer = "5" }
                        Image(IntroPane) { sprite = "'theme.'+string.lower('$Episode$')+'.intro-pane'" sublayer = "4" x = "490" y = "18" }
                        TextLabel(Title) { style = "TextLabel.Domcasual-10-Center" x = "360" y = "35" textDefault = "string.upper('$Episode$')+' OPEN'" }
                    }
                }
            }
        }
        
        Variant(OutroSpeech) {
            Case(American_Passed) {
                TextLabel(SpeechWinners) { style = "TextLabel.EpisodeOutro.Winners" x = "210" y = "390" }
                Repeat {
                    Variable(i) { start = "1" finish = "6" step = "1" }
                    Variable(y) { values = "(360, 420, 445, 470, 495, 520)" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "$y$" textDefault = "american.passed.$i$" }
                    }
                }
            }
            Case(Mexican_Passed) {
                TextLabel(SpeechWinners) { style = "TextLabel.EpisodeOutro.Winners" x = "210" y = "390" }
                Repeat {
                    Variable(i) { start = "1" finish = "6" step = "1" }
                    Variable(y) { values = "(360, 420, 445, 470, 495, 520)" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "$y$" textDefault = "mexican.passed.$i$" }
                    }
                }
            }
            Case(Russian_Passed) {
                TextLabel(SpeechWinners) { style = "TextLabel.EpisodeOutro.Winners" x = "210" y = "390" }
                Repeat {
                    Variable(i) { start = "1" finish = "6" step = "1" }
                    Variable(y) { values = "(360, 420, 445, 470, 495, 520)" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "$y$" textDefault = "russian.passed.$i$" }
                    }
                }
            }
            Case(Japanese_Passed) {
                TextLabel(SpeechLine1) { style = "TextLabel.EpisodeOutro.Winners" x = "210" y = "360" textDefault = "japanese.passed.1" }
                Repeat {
                    Variable(i) { start = "2" finish = "6" step = "1" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "330+30*$i$" textDefault = "japanese.passed.$i$" }
                    }
                }
            }
            Case(Hawaiian_Passed) {
                TextLabel(SpeechLine1) { style = "TextLabel.EpisodeOutro.Winners.Hawaiian" x = "210" y = "370" textDefault = "hawaiian.passed.1" }
                Repeat {
                    Variable(i) { start = "2" finish = "5" step = "1" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "335+35*$i$" textDefault = "hawaiian.passed.$i$" }
                    }
                }
            }
            Case(Japanese_Failed) {
                Repeat {
                    Variable(i) { start = "1" finish = "7" step = "1" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "340+25*$i$" textDefault = "japanese.failed.$i$" }
                    }
                }
            }
            Case(Hawaiian_Failed) {
                Repeat {
                    Variable(i) { start = "1" finish = "7" step = "1" }
                    Node {
                        TextLabel(SpeechLine$i$) { style = "TextLabel.EpisodeOutro.Text" x = "210" y = "340+25*$i$" textDefault = "hawaiian.failed.$i$" }
                    }
                }
            }
        }
        Repeat {
            Variable(i) { start = "0" finish = "3" step = "1" }
            Node {
                CharacterInTheLight(Character$i$)
            }
        }
        Repeat {
            Variable(i) { start = "0" finish = "7" step = "1" }
            Node {
                TextLabel(CharacterName$i$) { style = "TextLabel.EpisodeOutro.Names.NotPassed" x = "550" y = "305+30*$i$" }
                TextLabel(CharacterScore$i$) { style = "TextLabel.EpisodeOutro.Names.NotPassed" x = "710" y = "305+30*$i$" }
            }
        }
        PushButton(Close) {
            style = "PushButton.WholeScreen" sublayer = "1"
        }
        PushButton(Continue) {
            style = "PushButton.Continue" x = "730" y = "578"
        }
    }

    Composite(CharacterInTheLight) {
        Image(Character) { sublayer = "3" hotspot = "CenterBottom" }
        Image(Medallion) { sublayer = "1" x = "0" y = "-200" hotspot = "Center" }
        Image(Light) { sprite = "outro.light" sublayer = "4" x = "0" y = "40" hotspot = "CenterBottom" }
        TextLabel(Number) { style = "TextLabel.EpisodeOutro.WinnerNumber" sublayer = "0" x = "0" y = "-200" }
        TextLabel(Name) { style = "TextLabel.DomCasualCenter" x = "0" y = "-165" sublayer = "-1" }
    }

