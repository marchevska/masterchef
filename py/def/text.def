    text(def/42.def.intro.text) { str = "Summer vacation season has just begun, and tourists from all over the world are coming to Tiki Time today." }
    text(def/42.def.passed.text) { str = "Good work! Happy tourists will attract even more customers to our restaurant." }
    text(def/42.def.expert.text) { str = "Excellent work! Happy tourists will attract even more customers to our restaurant." }
    text(def/42.def.failed.text) { str = "We haven't made enough money today. Remember, the tourists are always the same: it's not difficult to keep them happy." }
    text(def/43.def.intro.title) { str = "Circus festival begins" }
    text(def/43.def.intro.text) { str = "The festival gathered thousands of circus friends. We have sponsored the young gymnasts show, and today many young people will dine at our restaurant." }
    text(def/43.def.passed.text) { str = "Good work! The day is over, and I'm going to see the circus show tonight." }
    text(def/43.def.expert.text) { str = "Excellent work! The day is over, and I'm going to see the circus show tonight." }
    text(def/43.def.failed.text) { str = "We haven't made enough money today. Kids may look nice, but don't let it go to your head: keep your eyes on each one of them." }
    text(def/44.def.intro.title) { str = "It's not time to relax yet" }
    text(def/44.def.intro.text) { str = "Nice work with young customers! If we continue this way, I have all chances to win the competition and this restaurant." }
    text(def/44.def.passed.text) { str = "Good work with the ladies! Everyone loved our cuisine, and we expect they'll recommend our restaurant to their friends." }
    text(def/44.def.expert.text) { str = "Excellent work with the ladies! Everyone loved our cuisine, and we expect they'll recommend our restaurant to their friends." }
    text(def/44.def.failed.text) { str = "We haven't made enough money today. Remember that old ladies and students are much more patient than business women." }
    text(def/45.def.intro.title) { str = "Merry Christmas!" }
    text(def/45.def.intro.text) { str = "Hawaii is the best place to meet July's Christmas holiday. Get ready to serve all kinds of customers today!" }
    text(def/45.def.passed.text) { str = "Good work! But I wish next Christmas I will be a customer, not a chef!" }
    text(def/45.def.expert.text) { str = "Excellent work! But I wish next Christmas I will be a customer, not a chef!" }
    text(def/45.def.failed.text) { str = "We haven't made enough money today. Pay more attention to the business women, they can bring you more tips." }
    text(def/46.def.intro.title) { str = "Food Critics again" }
    text(def/46.def.intro.text) { str = "Stacy Green, a famous food writer, gave a fabulous review about Tiki Time. Now judges from our jury are coming to check my work by themselves." }
    text(def/46.def.passed.text) { str = "Good work! If we passed this hardest day, anything else is in out powers, too!" }
    text(def/46.def.expert.text) { str = "Excellent work! If we passed this hardest day, anything else is in out powers, too!" }
    text(def/46.def.failed.text) { str = "We haven't made enough money today. I know it is hard, but try to keep your customers at the best mood." }
    text(def/47.def.intro.title) { str = "Older Persons Day" }
    text(def/47.def.intro.text) { str = "To attract grandmothers to our restaurant, today we've prepared small gifts for them. And of course, tourists and critics are coming too." }
    text(def/47.def.passed.text) { str = "Good work! I think that happy grandmothers and tourists will tell their friends about our place, so get ready for more job!" }
    text(def/47.def.expert.text) { str = "Excellent work! I think that happy grandmothers and tourists will tell their friends about our place, so get ready for more job!" }
    text(def/47.def.failed.text) { str = "We haven't made enough money today. Remember that old ladies and tourists can wait much longer than our jury members." }
    text(def/48.def.intro.title) { str = "Take a breath with toruists" }
    text(def/48.def.intro.text) { str = "A 1000*foot long cruise ship tied up at the island yesterday and brought us lots of new customers. Let's see if we can surprize them with our service." }
    text(def/48.def.passed.text) { str = "Good work! I hope that some of our today's customers will stay here for a while." }
    text(def/48.def.expert.text) { str = "Excellent work! I hope that some of our today's customers will stay here for a while." }
    text(def/48.def.failed.text) { str = "We haven't made enough money today. Remember, the tourists are always the same: it's not difficult to keep them happy." }
    text(def/49.def.intro.title) { str = "Another men*only day" }
    text(def/49.def.intro.text) { str = "The World Beach Volleyball Association holds the men's championship on the stadium right near our restaurant. All fans are welcome to dine today!" }
    text(def/49.def.passed.text) { str = "Good work! Do you think you're ready for the final day? Then, let's go!" }
    text(def/49.def.expert.text) { str = "Excellent work! Do you think you're ready for the final day? Then, let's go!" }
    text(def/49.def.failed.text) { str = "We haven't made enough money today. Gather yourself, there's only two more days left!" }
    text(def/50.def.intro.title) { str = "The final trial" }
    text(def/50.def.intro.text) { str = "Roll up your sleeves for the last and the longest competition day. Tonight we'll finally know who wins the contest and owns this restaurant." }
    text(def/50.def.passed.text) { str = "Good work! Now, let's check out who wins this round." }
    text(def/50.def.expert.text) { str = "Excellent work! Now, let's check out who wins this round." }
    text(def/50.def.failed.text) { str = "We haven't made enough money today. Make sure to serve every customer, don't let them get upset and just go away." }
